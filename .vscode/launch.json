    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "Launch Package",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${fileDirname}"
        },
        {
            "name": "Debug OKEx Buddy",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/backend/go/cmd",
            "buildFlags": [
                "-gcflags=all=-N -l",
                "-ldflags=all=-compressdwarf=false"
            ],
            "dlvFlags": [
                "--check-go-version=false"
            ],
            "envFile": "${workspaceFolder}/config/app.env",
            "env": {
                "PATH": "/usr/local/opt/go@1.23/bin:/usr/local/opt/go@1.23/libexec/bin:${env:PATH}",
                "USE_PROXY": "true",
                "PROXY_ADDR": "127.0.0.1:4781",
                "ENABLE_PUBLIC_WS": "false",
                "ENABLE_BUSINESS_WS": "true"
            },
            "args": [],
            "showLog": true,
            "console": "integratedTerminal"
        }
    ]
}